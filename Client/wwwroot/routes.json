{
  "routes": [
    {
      "route": "/api/AddPalabra",
      "allowedRoles": [ "updater" ]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "Unauthenticated",
      "statusCode": "302",
      "serve": "/login"
    }
  ]
}